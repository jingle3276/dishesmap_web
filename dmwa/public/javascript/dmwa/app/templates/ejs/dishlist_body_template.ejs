<div class="body-container">
	<% _.each(obj.dish_list, function(d) { %>
		<div class="row">
			<div class="list-group-item col-md-4 col-md-offset-4">
				<h3 class="list-group-item-heading text-primary"><%= d.getDishName() %></h3>
				<p class="list-group-item-text"><span class="label label-info"><%= d.getRestaurantName() %></span></p>
				<p class="list-group-item-text"><%= d.getLikes() %> loved it <span class="glyphicon glyphicon-heart"></span> | <%= d.getDistance() %> mi</p>
			</div>
		</div>
	<% }); %>
</div>
